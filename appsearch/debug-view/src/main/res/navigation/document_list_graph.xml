<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/document_list_graph"
    app:startDestination="@id/documentListFragment">

    <fragment
        android:id="@+id/documentListFragment"
        android:name="androidx.appsearch.debugview.view.DocumentListFragment"
        android:label="fragment_documents"
        tools:layout="@layout/fragment_document_list" >
        <action
            android:id="@+id/action_documentListFragment_to_documentFragment"
            app:destination="@id/documentFragment"
            app:popUpTo="@id/documentListFragment" />
    </fragment>

    <fragment
        android:id="@+id/documentFragment"
        android:name="androidx.appsearch.debugview.view.DocumentFragment"
        android:label="fragment_document"
        tools:layout="@layout/fragment_document" >
        <argument
            android:name="document_namespace"
            app:argType="string" />
        <argument
            android:name="document_id"
            app:argType="string" />
    </fragment>

</navigation>
