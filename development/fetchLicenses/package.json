{
  "name": "fetch-licenses",
  "version": "0.1.0",
  "description": "Fetch License files from Maven POM files",
  "main": "built/index.js",
  "scripts": {
    "clean": "rm -rf built",
    "compile": "node_modules/.bin/tsc",
    "watch": "node_modules/.bin/tsc --watch",
    "start": "node built/index.js",
    "debug": "npm run --silent watch & node_modules/.bin/nodemon --inspect built/index.js",
    "setupGcpProject": "gcloud config set project fetch-licenses",
    "deploy": "gcloud app deploy app.yaml"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/puppeteer": "^0.13.11",
    "express": "^4.17.3",
    "puppeteer": "^13.4.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "typescript": "^4.5.0"
  }
}
